/**
 * 16-bit bitwise Or:
 * for i = 0..15 out[i] = (a[i] or b[i])
 */

CHIP Or16 {
    IN a[16], b[16];
    OUT out[16];

    PARTS:
	Not(in=a, out=nota);
	Not(in=b, out=notb);
	And(a=a, b=b, out=andout);
	Not(in=andout, out=out);
}